{
  "extends": ["airbnb-base", "eslint:recommended", "plugin:node/recommended", "prettier"],
  "plugins": [
    "import",
    "node",
    "prettier"
  ],
  "rules": {
    "no-console": ["error", {
      "allow": ["warn", "error"]
    }],
    "prettier/prettier": "error"
  },
  "env": {
    "node": true,
    "es6": true
  }
}